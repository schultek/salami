{
  "name": "Salami",
  "version": "1.0.0",
  "description": "Halftone Generator with GCode export",
  "author": {
    "name": "Kilian Schulte" ,
    "email": "kilian@4xschulte.de"
  },
  "repository": "https://github.com/schultek/salami",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "build": "electron-builder -ml"
  },
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-packager": "^9.1.0"
  },
  "dependencies": {
    "base64-img": "^1.0.3",
    "electron-store": "^1.3.0",
    "get-pixels": "^3.3.0"
  },
  "build": {
    "appId": "schultek.salami",
    "productName": "Salami",
    "copyright": "Copyright Â© 2018 Kilian Schulte",
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": "dmg"
    },
    "linux": {
      "target": [
        {
        "target": "deb",
        "arch": ["x64"]
      }],
      "category": "Graphics"
    }
  }
}
